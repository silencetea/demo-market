{
	"name":"Chrome Extension demo",
	"version":"1.0.0",
	
	"manifest_version": 2,
	"description":"Hello chrome extension.",
	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},
	"web_accessible_resources":[
		"icon.png",
		"popup.js"
	],
	"content_scripts": [  
		{  
			"matches": ["*://open.10jqka.com.cn/clouds/*", "http://localhost:3000/clouds/*"],  
			"js": ["content.js"]  
		}  
	] 
}
